<p id="notice"><%= notice %></p>

<div class="container">
  <h3>Medical Records and Precriptions</h3>

  <table class="table table-striped">
    <thead>
    <tr>
      <th>Document Type</th>
      <th>URL</th>
      <th>File Name</th>
      <th colspan="3"></th>
    </tr>
    </thead>

    <tbody>
    <% @documents.each do |document| %>
      <tr>
      	<td><%= document.type %></td>
        <td><%= document.url %></td>
        <td><%= document.file_name %></td>
        </tr>
    <% end %>
    </tbody>
  </table>
</div>
